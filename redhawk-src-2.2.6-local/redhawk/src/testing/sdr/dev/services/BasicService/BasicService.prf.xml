<!DOCTYPE properties PUBLIC '-//JTRS//DTD SCA V2.2.2 PRF//EN' 'properties.dtd'>
<properties>
    <simple id="PARAM1"
            type="string">
        <value>ABCD</value>
        <kind kindtype="execparam"/>
    </simple>
    <simple id="PARAM2"
            type="short">
        <description>The meaning of life</description>
        <value>42</value>
        <kind kindtype="execparam"/>
    </simple>
    <simple id="PARAM3"
            type="float">
        <description>PI</description>
        <value>3.1459</value>
        <kind kindtype="execparam"/>
    </simple>
    <simple id="PARAM4"
            type="boolean">
        <value>False</value>
        <kind kindtype="execparam"/>
    </simple>
    <!-- No default for PARAM5, but we expect the DCD to override it -->
    <simple id="PARAM5"
            type="string">
        <kind kindtype="execparam"/>
    </simple>
    <!-- No default for PARAM5, but we expect the DCD to *NOT* override it -->
    <simple id="PARAM6"
            type="string">
        <kind kindtype="execparam"/>
    </simple>
</properties>
