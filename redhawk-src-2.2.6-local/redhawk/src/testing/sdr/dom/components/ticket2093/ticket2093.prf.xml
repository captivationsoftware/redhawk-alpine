<!DOCTYPE properties PUBLIC "-//JTRS//DTD SCA V2.2.2 PRF//EN" "properties.dtd">
<properties>
  <simple id="some_simple" mode="readwrite" type="short">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simplesequence id="some_sequence" mode="readwrite" type="short">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simplesequence>
  <struct id="some_struct" mode="readwrite">
    <simple id="some_float" mode="readwrite" type="float">
      <kind kindtype="configure"/>
      <action type="external"/>
    </simple>
    <configurationkind kindtype="configure"/>
  </struct>
  <structsequence id="some_struct_seq" mode="readwrite">
    <struct id="interim_id" mode="readwrite">
      <simple id="some_short" mode="readwrite" type="short">
        <kind kindtype="configure"/>
        <action type="external"/>
      </simple>
      <configurationkind kindtype="configure"/>
    </struct>
    <configurationkind kindtype="configure"/>
  </structsequence>
</properties>
