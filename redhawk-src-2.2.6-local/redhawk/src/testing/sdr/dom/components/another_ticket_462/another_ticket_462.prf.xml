<!DOCTYPE properties PUBLIC "-//JTRS//DTD SCA V2.2.2 PRF//EN" "properties.dtd">
<properties>
  <simple id="simple_prop" mode="readwrite" type="string">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simplesequence id="seq_prop" mode="readwrite" type="short">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simplesequence>
  <struct id="struct_prop" mode="readwrite">
    <simple id="prop_one" type="boolean"/>
    <simple id="prop_two" type="string"/>
    <simple id="prop_three" type="float"/>
    <configurationkind kindtype="configure"/>
  </struct>
  <structsequence id="struct_seq_prop" mode="readwrite">
    <struct id="tmp_struct">
      <simple id="prop_four" type="string"/>
      <simple id="prop_five" type="long"/>
      <simple id="prop_six" type="double"/>
    </struct>
    <configurationkind kindtype="configure"/>
  </structsequence>
</properties>
